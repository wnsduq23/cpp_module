#Variable
NAME	= megaphone
CC		= c++
CFLAGS	= -Wall -Wextra -Werror -std=c++98
DEL		= rm -rf

# srcs

# rules
all : $(NAME)

clean : 

fclean :
	$(DEL)
re :
	make fclean
	make all
.PHONY : all re fclean clean

